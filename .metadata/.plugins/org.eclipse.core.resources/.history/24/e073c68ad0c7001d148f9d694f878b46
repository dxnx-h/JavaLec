package chap12_nestedclass.clazz;

public class Add implements Calc {

	@Override
	public void calculator(int a, int b) {
		// TODO Auto-generated method stub
		Calc calc = new Calc() {

			@Override
			public void calculator(int a, int b) {
				System.out.println("결과는 " + (a + b));
			}

		};
		c
		calc.calculator(10, 20);
	}
}
