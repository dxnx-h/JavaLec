package chap10_abstraction.academy;

	public class englishAcademy extends nowAcademy {
	
		public void teach() {
			System.out.println(teacher + "명이" 
							+ group +"개 반을 가르친다.");
		}

		public void homework() {
			System.out.println(student + "명이 숙제를 한다");
		}

		public void rest() {
			System.out.println("english Academy는 쉰다");
		}

		@Override
		public void attendance(int lastSubject) {
			System.out.println(subject + "개는 휴강한다");
			
			super.subject= lastSucject;
			
		}

}
