package chap18_enum.enums;

public enum CusMonth {
	//지정하는 상수는 모두 Static final로 지정되고 값을 변경할 수 없다. 
	//2. 이제 넣음 값
	JAN(1), FEB(2), MAR(3), APR(4), MAY(5), JUN(6), JUL(7), AUG(8), 
	SET(9), OCT(10), NOV(11), DEC(12);
	
	private final int CUS_MONTH;
	
	//1. 생성자 먼저 생성
	CusMonth(int month) {
		CUS_MONTH = month;
	}
	
	public int getMonth() {
		return CUS_MONTH;
	}
	
}
