package chap16_collectionFramework;

import java.util.ArrayList;
import java.util.List;

import chap06_class.Car.Car;

public class _07_iterator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//Map<String, String> map;
		List<Car> cList = new ArrayList<Car>();
		
		Car car = new Car("현대", "제네시스", "은색", 5000);
		Car car = new Car("현대", "아반떼", "흰색", 3000);
		Car car = new Car("현대", "소나타", "검정색", 4000);
		Car car = new Car("기아", "ev6", "파란색", 4000);
		Car car = new Car("기아", "모하비", "빨간색", 5000);
		
		cList.add(car);
	}

}
