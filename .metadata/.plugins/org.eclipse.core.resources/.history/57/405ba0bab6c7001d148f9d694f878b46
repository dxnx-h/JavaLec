package chap12_nestedclass.clazz;

public class NestedClass {
	//인스턴스 중첩 클래스. 
	public class Mul {
		int num1 = 10; //int num1 = 10;
		int num2 = 20;
		
		int result = num1 * num2;
	}
	
	public static class Div {
		//스태틱클래스는 외부 클래스 객체 없이도 바로 만들수 있음.
		public int num1 = 10;
		public int num2 = 5;
		
		public int result = num1 / num2;
	}
}

