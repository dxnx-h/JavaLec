package chap18_enum.enums;

public enum Season {
	SPRING("봄") {
		public void seasonMon() {
			
		}
	}
	
	
	
	, SUMMER("여름")
	
	
	
	, FALL("가을")
	
	
	
	, WINTER("겨울");

	
	private String season;
	
	Season(String season) {
		this.season = season;
	}

	public String getSeason() {
		return season;
	}

	public abstract void seasonMon(); //1. 추상메소드 먼저 선언
}
