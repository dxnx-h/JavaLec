package chap11_interface;

//클래스가 인터페이스를 상속받을 때는 impliments 키워드를 사용한다. 
//인터페이스를 상속받아 추상메소드 중 일부만 구현항려면 추상클래스로 선언한다.
public abstract abstract /*abstract*/ class HighBuilding implements building {

	@Override
	public void build() {
		// TODO Auto-generated method stub
		System.out.println();
		
	}

	@Override
	public void turnOnAircon() {
		// TODO Auto-generated method stub
		System.out.println("에어컨을 가동하다");
		
	}

	@Override
	public int getParkingPrice(int time) {
		// TODO Auto-generated method stub
		int totalPrice = time * 100;0
		return 0;
	}

	
	
}
