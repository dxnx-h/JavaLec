package chap00_homework._20230314;

public class _20230314_basic_02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		/*Q2) 관계연산자(<, >) 두 개를 사용하여 int a = 10; int b = 20; 의 
		 *관계를 다음과 같이 출력하세요.
    true
    false
		 */
		int a = 10;
		int b = 20;
		boolean num = false;
				
		if(a<b) {
			num = true;
		}
		
		if(num == true) {
			System.out.println("true");
		} else {
			System.out.println("false");
		}
		
			
		if(num == false) {
			System.out.println("true");
		} else {
			System.out.println("false");
		}
	}

}
