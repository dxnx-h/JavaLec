package chap12_nestedclass;

class NestedClass {
	class Add {
		int num1;
		int num2;
		
		int result;
		
		int add() {
			result = num1 + num2;
			return result;
		}
	}
	
	static class Sub {
		static int num3;
		static int num4;
		
		static int result1;
		static int sub() {
			result1 = num3 - num4;
			return result1;
		}
	}
}
public class _03_instanceOfNestedClass {

	public static void main(String[] args) {
		//같은 파일 내라 private 말곤 동작을 안할것임
		
		//1. 정적 중첩 클래스의 사용
		

	}

}
