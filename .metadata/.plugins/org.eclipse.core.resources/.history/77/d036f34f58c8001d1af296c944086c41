package chap13.objectarry.clazz2;

public class Mathmetics implements ILecture1 {

	private String subject;
	private int studentCnt;
	private int lectureTime;
	
	public Mathmetics(String subject, int studentCnt, int lectureTime) { //생성자
		this.subject = subject;
		this.studentCnt = studentCnt;
		this.lectureTime = lectureTime;
		
	}
	@Override
	public void proceedLecture() {
		// TODO Auto-generated method stub
		System.out.println("수학수업을 30명의 학생이 듣습니다."
				  + " 수업시간은 60분입니다."); 
	}

}
