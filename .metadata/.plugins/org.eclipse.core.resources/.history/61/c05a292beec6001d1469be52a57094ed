package chap09_polymorphism.product;

public class Lgtv extend Tv {
	
	public void powerOn() {
		System.out.println("제조사: Lg");
		super.powerOn();
	}
	
	public void pewerOff() {
		System.out.println("제조사: Lg");
		super.pewerOff();
	}
	
	public void operate(int channel) {
		super.operate(channel);
	}

}

