package chap00_homework._20230321_Mid.clazz;

public class homeCalculator {
	public static class homeAdd {
		static int a;
		static int b;
		

		public int getA() {
			return a;
		}

		public void setA(int a) {
			this.a = a;
		}

		public int getB() {
			return b;
		}

		public void setB(int b) {
			this.b = b;
		}



		public static int result = a + b;
	}
	
	public static class homeSub {
		static int a = 4;
		static int b = 3;
		public static int getA() {
			return a;
		}
		public static void setA(int a) {
			homeSub.a = a;
		}
		public static int getB() {
			return b;
		}
		public static void setB(int b) {
			homeSub.b = b;
		}
		public static int getResult() {
			return result;
		}
		public static void setResult(int result) {
			homeSub.result = result;
		}
		public static int result = a - b;
	}
	
	public static class homeMul {
		static int a = 5;
		static int b = 6;
		public static int getA() {
			return a;
		}
		public static void setA(int a) {
			homeSub.a = a;
		}
		public static int getB() {
			return b;
		}
		public static void setB(int b) {
			homeSub.b = b;
		}
		public static int getResult() {
			return result;
		}
		public static void setResult(int result) {
			homeSub.result = result;
		}
		public static int result = a * b;
	}
	
	public static class homeDiv {
		int a = 16;
		int b = 8;
		
		int result = a / b;

		public int getA() {
			return a;
		}

		public void setA(int a) {
			this.a = a;
		}

		public int getB() {
			return b;
		}

		public void setB(int b) {
			this.b = b;
		}

		public int getResult() {
			return result;
		}

		public void setResult(int result) {
			this.result = result;
		}
		
	}
	
}
